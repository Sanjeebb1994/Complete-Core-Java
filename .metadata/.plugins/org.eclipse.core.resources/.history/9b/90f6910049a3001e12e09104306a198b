package Collection;

import java.util.Stack;

public class DemoStack {
	public static void main(String[] args) {
		Stack s=new Stack();
		s.add("Sanjeeb");
		s.add(12.34f);
		s.add('E');
		s.add(20);
		s.add(20);
		s.add(true);
		s.add(null);
		System.out.println("Length of stack is:: "+s.size());
		System.out.println(s);
	}
}
