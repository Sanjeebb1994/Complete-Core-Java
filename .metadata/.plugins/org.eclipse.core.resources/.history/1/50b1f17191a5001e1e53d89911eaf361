package Collection;

import java.util.Hashtable;

public class DemoHashTable {
	public static void main(String[] args) {
		Hashtable<Integer, String> ht=new Hashtable<Integer, String>();
		ht.put(1, "Sanjeeb");
		ht.put(2, "Shyam");
		ht.put(3, "Rajeeb");
		ht.put(4, "Raju");
		System.out.println(ht);
		
	}
}
